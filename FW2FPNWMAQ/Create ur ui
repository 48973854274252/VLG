local colors = {
    SchemeColor = Color3.fromRGB(255, 165, 0),  
    Background = Color3.fromRGB(40, 40, 40),    
    Header = Color3.fromRGB(30, 30, 30),         
    TextColor = Color3.fromRGB(255, 255, 255),
    ElementColor = Color3.fromRGB(50, 50, 50)
}

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/48973854274252/VLG/refs/heads/main/.gitignore"))()
local Window = Library.CreateLib("Main Title", colors)

local Tab1 = Window:NewTab("Main Tabs")
local Section1 = Tab1:NewSection("Tab Title For Ur Scripts info Bottom is ur scripts buttons")

Section1:NewButton("Ur Scripts Title", "Info for button", function()
    print("Test")
end)

local Tab2 = Window:NewTab("Main Tabs")
local Section2 = Tab2:NewSection("Title bottomid Scripts")

Section2:NewButton("Script", "Info for button", function()
    print("Test")
end)

local Tab3 = Window:NewTab("Window Tab title")
local Section3 = Tab3:NewSection("Scripts Title bellow scripts")

Section3:NewButton("Info", "Info for button", function()
    print("Test")
end)
